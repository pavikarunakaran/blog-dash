{
  "name": "blog-dash",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "jest": {
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "^msw/node$": "<rootDir>/node_modules/msw/lib/node/index.js",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mswjs/interceptors": "^0.38.2",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "msw": "^1.2.3",
    "next": "15.3.0",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@reduxjs/toolkit": "^2.6.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-fetch": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-redux": "^9.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.1",
    "typescript": "^5"
  }
}
